{
  "name": "Traslado",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "fechaAsignacion": {
      "type": "date"
    },
    "fechaEstimada": {
      "type": "date",
      "required": true
    },
    "fechaFin": {
      "type": "date"
    },
    "estado": {
      "type": "string",
      "default": "pendiente de retiro"
    },
    "tipo": {
      "type":"string"
    },
    "volumenTotal": {
      "type":"number"
    },
    "puntaje": {
      "type":"number"
    },
    "distancia": {
      "type":"number"
    },
    "descripcion": {
      "type":"string"
    },
    "peso": {
      "type":"number"
    },
    "fechaVencimientoInvitacion": {
      "type": "date",
      "default": "$now",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "enviosParaBeneficiario": {
      "type": "hasOne",
      "model": "envioParaBeneficiario",
      "foreignKey": "idTraslado"
    },
    "envioAPartirDeDonacion": {
      "type": "hasOne",
      "model": "donacion",
      "foreignKey": "idTrasladoQueLaIncluyeEnUnEnvio"
    }
  },
  "acls": [],
  "methods": {}
}
