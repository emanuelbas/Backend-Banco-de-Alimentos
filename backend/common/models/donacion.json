    "validateUpsert": true
  },
  "properties": {
    "fechaRegistro": {
      "type": "date",
      "default": "$now",
      "required": true
    },
    "numero": {
      "type": "number",
      "unique": true,
      "required": true
    },
    "estado": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
   "descripcionDetallada": {
     "type": "hasOne",
     "model": "DescripcionDetallada",
     "foreignKey": "idDonacion"
   },
   "descripcionGeneral": {
     "type": "hasOne",
     "model": "DescripcionGeneral",
     "foreignKey": "idDonacion"
   },
   "traslado": {
     "type": "hasOne",
     "model": "Traslado",
     "foreignKey": "idDonacionTrasladadaAlBanco"
   }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
